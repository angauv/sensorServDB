<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_sensor_servlet_db"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.a00839270.sensorservdb.MainActivity">

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/hello"
        android:layout_above="@+id/button"
        android:layout_alignLeft="@+id/outputTxt"
        android:layout_alignStart="@+id/outputTxt" />

    <Button
        android:id="@+id/button"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/button"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/outputTxt"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:textStyle="bold"
        android:layout_below="@+id/button"
        android:layout_alignLeft="@+id/textView2"
        android:layout_alignStart="@+id/textView2" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:text="Xaccel Axis: "
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/TextViewYaccel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView2"
        android:layout_alignLeft="@+id/TextViewXaccel"
        android:text="Unknown"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/TextViewZaccel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView3"
        android:layout_alignLeft="@+id/TextViewYaccel"
        android:text="Unknown"
        android:textAppearance="?android:attr/textAppearanceLarge" />


    <TextView
        android:id="@+id/TextViewZgyro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView6"
        android:layout_alignLeft="@+id/TextViewYgyro"
        android:text="Unknown"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/TextViewXaccel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Unknown"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/textView1"
        android:layout_toEndOf="@+id/textView1"
        android:layout_marginLeft="72dp"
        android:layout_marginStart="72dp" />

    <TextView
        android:id="@+id/TextViewYgyro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Unknown"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_alignBaseline="@+id/textView5"
        android:layout_alignBottom="@+id/textView5"
        android:layout_alignLeft="@+id/TextViewXgyro"
        android:layout_alignStart="@+id/TextViewXgyro" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Zgyro Axis: "
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_marginBottom="16dp"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/textView4"
        android:layout_alignEnd="@+id/textView4" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ygyro Axis: "
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_above="@+id/textView6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="13dp" />

    <TextView
        android:id="@+id/TextViewXgyro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Unknown"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_marginBottom="14dp"
        android:layout_above="@+id/TextViewYgyro"
        android:layout_alignLeft="@+id/TextViewZaccel"
        android:layout_alignStart="@+id/TextViewZaccel" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Xgyro Axis: "
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_alignBaseline="@+id/TextViewXgyro"
        android:layout_alignBottom="@+id/TextViewXgyro"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        android:text="Zaccel Axis: "
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_below="@+id/textView2"
        android:layout_toLeftOf="@+id/TextViewXaccel"
        android:layout_toStartOf="@+id/TextViewXaccel" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        android:text="Yaccel Axis: "
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_below="@+id/textView1"
        android:layout_toLeftOf="@+id/TextViewXaccel"
        android:layout_toStartOf="@+id/TextViewXaccel" />

</RelativeLayout>
